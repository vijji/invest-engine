{
  "name": "vite-vue-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/server/main.ts -w src/server",
    "start": "NODE_ENV=production ts-node src/server/main.ts",
    "start-script": "NODE_ENV=production ts-node src/server/scripts/test.ts",
    "test": "NODE_ENV=production ts-node src/server/scripts/test.ts",
    "build": "vite build",
    "lint": "eslint .  --ext .js",
    "lint-fix": "eslint --fix .  --ext .js",
    "lint-ts": "eslint .  --ext .ts",
    "lint-ts-fix": "eslint --fix .  --ext .ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "node": "^19.8.1",
    "node-html-parser": "^6.1.5",
    "ts-node": "^10.9.1",
    "typescript": "*",
    "vite-express": "*",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@baggie/math": "^1.5.0",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.10.0",
    "nodemon": "^2.0.20",
    "vite": "^4.0.4",
    "vue-tsc": "^1.0.24"
  }
}
